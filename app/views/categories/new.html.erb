<%= form_with model: @category , url: categories_path do |form| %>


  <div >
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div >
  <%= form.label :icon, style: "display: block" %>
    <%#= form.text_field :icon %>
    <%= form.select :icon, [['🎲'], ['🚘'], ['🛒'], ['🍕'], ['📚'], ['🎞️'], ['🎶'], ['🍎'], ['👾'], ['✍🏽'], ['🏃🏽'], ['❤️'], ['⭐'], ['🐶'], ['🌳'], ['🌎'], ['🌙'], ['💧'], ['☀️'], ['🌹'], ['🏈'], ['🥊'], ['✈️'], ['🏕️'], ['🚲'], ['🔨'], ['📆'], ['📎'], ['🔒'], ['📌'], ['✂️'], ['🕙'], ['🧹']] %>
  </div>

  <div >
    <%= form.submit %>
  </div>
  <% end %>
